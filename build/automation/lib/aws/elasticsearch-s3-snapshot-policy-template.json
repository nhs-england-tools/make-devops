{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Effect": "Allow",
      "Action": ["s3:ListBucket"],
      "Resource": ["arn:aws:s3:::BUCKET_TO_REPLACE"]
    },
    {
      "Effect": "Allow",
      "Action": [
        "iam:PassRole",
        "s3:DeleteObject",
        "s3:GetObject",
        "s3:PutObject"
      ],
      "Resource": ["arn:aws:s3:::BUCKET_TO_REPLACE/*"]
    }
  ]
}
